if [ "$CONFIG_STALDRV" = "y" ]; then
  tristate '  Stallion EasyIO or EC8/32 support' CONFIG_STALLION n
  tristate '  Stallion EC8/64, ONboard, Brumby support' CONFIG_ISTALLION n
fi
