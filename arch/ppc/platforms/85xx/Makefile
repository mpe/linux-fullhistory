obj-$(CONFIG_MPC8555_CDS)	+= mpc85xx_cds_common.o
obj-$(CONFIG_MPC8560_ADS)	+= mpc85xx_ads_common.o mpc8560_ads.o
obj-$(CONFIG_SBC8560)		+= sbc85xx.o sbc8560.o
obj-$(CONFIG_STX_GP3)		+= stx_gp3.o
