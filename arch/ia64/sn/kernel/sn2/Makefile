EXTRA_CFLAGS    := -DLITTLE_ENDIAN

.S.s:
	$(CPP) $(AFLAGS) $(AFLAGS_KERNEL) -o $*.s $<
.S.o:
	$(CC) $(AFLAGS) $(AFLAGS_KERNEL) -c -o $*.o $<

all: sn2.a

O_TARGET        = sn2.a

obj-y          = cache.o iomv.o sn2_smp.o

clean::

include $(TOPDIR)/Rules.make
